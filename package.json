{
  "name": "compare-css",
  "version": "1.0.1",
  "description": "Tests whether two pieces of CSS text are equivalent in browser",
  "keywords": [
    "css",
    "compare",
    "equivalent",
    "compare css"
  ],
  "browser": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "author": "Akash Srivastava <akash.srivastava.1911@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/akashdotsrivastava/compare-css.git"
  },
  "bugs": {
    "url": "https://github.com/akashdotsrivastava/compare-css/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/akashdotsrivastava/compare-css/blob/main/LICENSE.md"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "ramda": "^0.28.0"
  },
  "devDependencies": {
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1"
  }
}
